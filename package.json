{
  "name": "@xmlking/nativescript-ngx-microsoftband",
  "version": "0.1.0",
  "description": "Microsoft Band Plugin for NativeScript with Angular.",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.4.1",
      "ios": "2.4.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xmlking/nativescript-ngx-microsoftband.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "ng",
    "ngx",
    "ng2",
    "iot",
    "fitness",
    "healthare",
    "nativeScript",
    "microsoftBand"
  ],
  "author": "Sumanth Chinthagunta <xmlking@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xmlking/nativescript-ngx-microsoftband/issues"
  },
  "homepage": "https://github.com/xmlking/nativescript-ngx-microsoftband#readme",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf src/platforms",
    "demo.ios": "yarn run preparedemo; cd src; tns run ios --emulator --justlaunch",
    "demo.android": "yarn run preparedemo; cd src; tns run android --emulator --justlaunch",
    "preparedemo": "yarn run build; cd src; tns plugin remove @xmlking/nativescript-ngx-microsoftband; tns plugin add ..;",
    "setup": "cd src && yarn && cd .. && yarn"
  },
  "devDependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "@angular/core": "^2.4.4",
    "nativescript-angular": "next",
    "rxjs": "^5.0.3",
    "tns-core-modules": "next",
    "tns-platform-declarations": "2.4.4",
    "typescript": "~2.1.5",
    "zone.js": "~0.7.6"
  }
}
